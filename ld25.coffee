deep_array_copy = (a) ->
  ret = new Array
  for i in a
    ret.push(i[..])
  ret

make_damage_array = (a) ->
  ret = new Array
  for i in a
    ret.push((j*500 for j in i))
  ret

class City
  constructor: (level) ->
    @level = deep_array_copy(level)
    @damage = make_damage_array(level)
    @num_buildings_left = @get_buildings_left()

  draw: () ->
    ctx.fillStyle = "black"
    for row, y in @level
      for col, x in row
        switch col
          when 1
            ctx.fillStyle = "#111"
            ctx.fillRect(x * BS, y * BS, BS, BS)
          when -2, -3
            ctx.fillStyle = "#999"
            ctx.fillRect(x * BS, y * BS, BS, BS)
          when -4
            ctx.fillStyle = "#a33"
            ctx.fillRect(x * BS, y * BS, BS, BS)

  get_buildings_left: () ->
    sum = 0
    for i in @level
      sum += i.reduce((a, b) -> a + b)
    sum

  take_damage: (x, y, amount) ->
    if @damage[y][x] > 0
      @damage[y][x] -= amount
      if @damage[y][x] <= 0
        @level[y][x] = rand(-4, 1)
        @num_buildings_left--
        particles.push(new Debris(x * BS, y * BS))
        particles.push(new Debris(x * BS, y * BS))
        particles.push(new Debris(x * BS, y * BS))
        gold++

class Unit
  constructor: (@color, @x, @y) ->
    @health = @max_health = 50
    @target = null
    @velocity = 6
    @dir = {x: 0, y: 0}
    @attack_range = 20
    @attack_cooldown = 0
    @attack_cooldown_remaining = 0
    @strength = 30

  draw: () ->
    ctx.fillStyle = @color
    ctx.fillRect(@x, @y, 4, 4)
    ctx.strokeStyle = '#222'
    ctx.strokeRect(@x, @y, 4, 4)

  _find_target: (city) ->
    min = 999999
    target_x = -1
    target_y = -1
    for row, y in city.level
      for col, x in row
        if col == 1
          tmp_x = x * BS
          tmp_y = y * BS
          distance = Math.pow(@x - tmp_x, 2) + Math.pow(@y - tmp_y, 2)
          if distance < min and Math.random() > 0.3
            target_x = x
            target_y = y
            min = distance
    if target_x != -1
      @target = {
        x: target_x * BS + BS, y: target_y * BS + BS,
        rx: target_x, ry: target_y
      }
      @dir = make_unit_vector(@x, @y, @target.x, @target.y)
    else
      @target = {x: 0, y: 0, rx: 0, ry: 0}
      @dir = {x: 0, y: 0}

  _can_attack_target: () ->
    rem_x = @target.x - @x
    rem_y = @target.y - @y
    dist_squared = rem_x * rem_x + rem_y * rem_y
    return dist_squared < @attack_range

  _attack: (city) ->
    city.take_damage(@target.rx, @target.ry, @strength)
    particles.push(new Debris(@target.x, @target.y))
    @attack_cooldown_remaining = @attack_cooldown

  move: (city) ->
    if @target is null or city.level[@target.ry][@target.rx] <= 0
      @_find_target(city)
    if @_can_attack_target()
      if @attack_cooldown_remaining == 0
        @dir.x = @dir.y = 0
        @_attack(city)
      else
        @attack_cooldown_remaining--
    else
      if within(@x, @target.x, @dir.x * @velocity)
        @x = @target.x
        @dir.x = 0
      else
        @x += @dir.x * @velocity
      if within(@y, @target.y, @dir.y * @velocity)
        @y = @target.y
        @dir.y = 0
      else
        @y += @dir.y * @velocity

within = (pos, target_pos, velocity) ->
  return (Math.abs(target_pos - pos) < Math.abs(velocity))

