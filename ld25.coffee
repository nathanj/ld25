draw = () ->
  ctx.clearRect(0, 0, WIDTH, HEIGHT)
  switch state
    when STATE_OVERWORLD then draw_overworld()
    when STATE_RECRUIT then draw_recruit()
    when STATE_BATTLE then draw_battle()
    when STATE_BATTLE_OVER then draw_battle_over()
    when STATE_GAME_OVER then draw_game_over()

update_battle = () ->
  army.move(city)
  p.move() for p in particles
  particles = particles.filter((p) -> !p.done)
  if army.has_a_unit()
    hours++
    if hours == 50
      day++
      hours = 0
  if city.num_buildings_left == 0
    cities_destroyed++
    state = STATE_BATTLE_OVER
    if cities_destroyed == 5
      state = STATE_GAME_OVER

update = () ->
  switch state
    when STATE_BATTLE then update_battle()
  draw()

